package com.wipro.employee;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        Employee emp=new Employee();  // constructor automatically called
       
        //constructor - set default values to instance variable 
        
        System.out.println(emp.id);  // 0
        System.out.println(emp.name); // null
        System.out.println(emp.getSalary());//0.0
        System.out.println(emp.getPhone()); // 0
        
    }
}
