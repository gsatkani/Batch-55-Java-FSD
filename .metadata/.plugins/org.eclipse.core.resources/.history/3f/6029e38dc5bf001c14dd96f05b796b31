package com.wipro.basic;

public class Test {
	
	public static void main(String[] args) {
		
		// Binary Op   a+b
		// Unary Op   a++  ++a -> Increment Operators
		// PreIncrement  a++ -> increment a by 1 a=10 a++ -> 11 
		
		int a=10;
		System.out.println(a++);//10
		System.out.println(a); //11
		
		
		
		
	}

}
