package com.wipro.basic;

public class Employee {
	
	String empName;
	String company;
	
	
	
	public static void main(String[] args) {
		Employee emp1=new Employee();
		emp1.empName="Abc";
		emp1.company="Wipro";
		
		
		Employee emp2=new Employee();
		emp2.empName="Def";
		emp2.company="Wipro";
		
	}
	
	

}
